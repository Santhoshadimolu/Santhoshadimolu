1) eligible for vote or not
#include <iostream>
using namespace std;
int main() {
    int age=9;
    if(age>=18)
    {
   std:: cout<<"Eligible for vote"<<std::endl;
    }
    else
    {
   std::  cout<<"not eligible for vote "<<" eligible after "<< 18-age<< " years"<<std::endl;
    }
    return 0;
}


2) multiplication table 
#include<iostream>
using namespace std;
int main(){
    int i,n=5,p;
    for(i=1; i<=10;i++){
    p=n*i;
    cout<<n<<"*"<<i<<"="<<p<<endl;
    }
    return 0;
}

3)swaping
#include<iostream>
using namespace std;

int main(){
    int a = 5;
    int b = 9;
    int temp;
    temp = a;
    a = b;
    b = temp;
    
    cout << "After swapping, a = " << a << " and b = " << b << endl;
    
    return 0;
}


4)odd or even
#include<iostream>
using namespace std;

int main(){
   int a=10;
   if(a%2==0)
   cout<<"it is an even number"<<endl;
   else
   cout<<"it is an odd number"<<endl;
    return 0;
}



5) even sum and odd sum
#include<iostream>
using namespace std;
int main(){
  int odd_sum=0,even_sum=0,o_count=0,e_count=0,i;
  for(i=1;i<=10;i++)
      if(i%2==0)
  {
      even_sum=even_sum+i;
      e_count++;
  }
  else
  {
      odd_sum=odd_sum+i;
  o_count++;
  }
  
  cout<<"the even_sum="<<even_sum<<endl;
  cout<<"the odd_sum="<<odd_sum<<endl;
  cout<<"the even_count="<<e_count<<endl;
  cout<<"the odd_count="<<o_count<<endl;
    return 0;
}


6)palindrome
#include<iostream>
using namespace std;
int main(){
 int num=121,reversed =0,rem, originalnum;
 originalnum=num;
 while(num!=0)
 {
     rem = num%10;
     reversed=reversed*10+rem;
     num/=10;
 }
 if(originalnum==reversed)
 {
     cout<<originalnum<<" is a palindrome"<<endl;
 }
 else
 {
     cout<<originalnum<<"is not a palindrome"<<endl;
 }
    return 0;
}

7)armstrong number
#include<iostream>
using namespace std;
int main()
{
int num=153,originalnum,rem,result=0;
originalnum=num;
while(originalnum!=0){
 rem = originalnum%10;
    result+=rem*rem*rem;
originalnum/=10;}
    if(result==num)
 {cout<<num<<"is a arm strong number"<<endl;
    }
else
    {
 cout<<"is not an arm strong number"<<endl;
    }
return 0;
}

8)fibanocci series
#include<iostream>
using namespace std;

int main() {
    int i, n1 = 0, n2 = 1, n = 10;
    int nt;
    
    cout << n1 << " " << n2 << " ";
    
    for (i = 2; i < n; i++) {
        nt = n1 + n2;
        cout << nt << " ";
        n1 = n2;
        n2 = nt;
    }
    
    return 0;
}

9)factorial
#include<iostream>
using namespace std;
int main(){
    int i,n=5,fact=1;
    for(i=1;i<=n;i++)
    {
        fact*=i;
    }
    cout<<"factorial="<<fact;
    return 0;
}


